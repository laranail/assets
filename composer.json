{
  "name": "laranail/assets",
  "description": "A highly opinionated HTML assets management library for the Laravel framework.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "simtabi",
    "simtabi cms",
    "simtabi platform",
    "simtabi git commit checker",
    "git",
    "commit checker"
  ],
  "homepage": "https://simtabi.com",
  "authors": [
    {
      "name": "Imani Manyara",
      "role": "Developer",
      "homepage": "https://simtabi.com"
    },
    {
      "name": "Easter Mukora",
      "role": "Developer",
      "homepage": "https://simtabi.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "laravel/framework": ">=v9.52.4",
    "laravel/pint": ">=1.7.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.6.5",
    "mockery/mockery": ">=1.0"
  },
  "autoload": {
    "psr-4": {
      "Simtabi\\Laranail\\Assets\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Simtabi\\Laranail\\Assets\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Simtabi\\Laranail\\Assets\\Providers\\AssetsServiceProvider"
      ],
      "aliases": {
        "AssetsFacade": "Simtabi\\Laranail\\Assets\\Facades\\AssetsFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
